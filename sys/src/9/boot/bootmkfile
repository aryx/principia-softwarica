MKSHELL=$PLAN9/bin/rc

BOOTDIR=../boot
BOOTLIB=$BOOTDIR/libboot.a$O

BOOTFILES=\
	aux.$O\
	boot.$O\
	bootauth.$O\
	bootcache.$O\
	bootip.$O\
	embed.$O\
	local.$O\
	paq.$O\
	parts.$O\
	printstub.$O\
	settime.$O\

#pad: nothere: testboot.c

$BOOTLIB(%.$O):N:	%.$O

$BOOTLIB:	${BOOTFILES:%=$BOOTLIB(%)}
	names=`{membername $newprereq}
	iar vu $BOOTLIB $names
	rm $names

$BOOTFILES:	$BOOTDIR/boot.h

%.$O:	$BOOTDIR/%.c
	$CC -I$BOOTDIR $CFLAGS $BOOTDIR/$stem.c
