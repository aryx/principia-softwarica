+				typed[i] = 0;
+				break;
+			}
+		}
+		if(i == sizeof typed)
+			continue;
+		if(passtokey(crypted, typed) == 0)
+			continue;
+		seek(fd, 0, 0);
+		if(read(fd, key, DESKEYLEN) != DESKEYLEN){
+			print("init: can't read key; insecure\n");
+			return;
+		}
+		if(memcmp(crypted, key, sizeof key))
+			continue;
+		/* clean up memory */
+		memset(crypted, 0, sizeof crypted);
+		memset(key, 0, sizeof key);
+		return;
+	}
