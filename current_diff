-	int s;
-	volatile ulong instr[2];
-	Pvfu fp;
-
-	/* fpu might be off and this VMSR might enable it */
-	s = splhi();
-	setupfpctlop(instr, 0xeee00010, fpreg);		/* VMSR, Rt is R0 */
-	fp = (Pvfu)instr;
-	fp(val);
-	coherence();
-	splx(s);
-}
