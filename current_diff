-		dumpstack();
-		panic("fprd: cpu%d fpu off", m->machno);
-	}
-	s = splhi();
-	/*
-	 * VMRS.  return value will be in R0, which is convenient.
-	 * Rt will be R0.
-	 */
-	setupfpctlop(instr, 0xeef00010, fpreg);
-	fp = (Pufv)instr;
-	r = fp();
-	splx(s);
-	return r;
-}
