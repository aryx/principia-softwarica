-void
-fpstuck(uintptr pc)
-{
-	if (m->fppc == pc && m->fppid == up->pid) {
-		m->fpcnt++;
-		if (m->fpcnt > 4)
-			panic("fpuemu: cpu%d stuck at pid %ld %s pc %#p "
-				"instr %#8.8lux", m->machno, up->pid, up->text,
-				pc, *(ulong *)pc);
-	} else {
-		m->fppid = up->pid;
-		m->fppc = pc;
-		m->fpcnt = 0;
-	}
-}
