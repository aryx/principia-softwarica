TOP=../../../../..
<$TOP/mkfiles/$objtype/mkfile

TARG=audio

OFILES=\
	audiofs.$O\
	audiosub.$O\
	audio.$O\
	audioctl.$O\

HFILES=\
	audio.h\
	audioctl.h\
	../lib/usb.h\

LIB=../lib/usb.a$O

<$TOP/mkfiles/mkone

BIN=/$objtype/bin/usb

CFLAGS=-I../lib $CFLAGS

$LIB:
	cd ../lib
	mk install
	mk clean
